		return nil, err
	}

	var rowid, changes  /*line :2123:21*/_Ctype_longlong /*line :2123:31*/
	rv := func() _Ctype_int{ _cgo0 := /*line :2124:37*/s.s; var _cgo1 *_Ctype_longlong = /*line :2124:42*/&rowid; var _cgo2 *_Ctype_longlong = /*line :2124:50*/&changes; _cgoCheckPointer(_cgo0, nil); return /*line :2124:59*/_Cfunc__sqlite3_step_row_internal(_cgo0, _cgo1, _cgo2); }()
	if rv != ( /*line :2125:11*/_Ciconst_SQLITE_ROW /*line :2125:22*/) && rv != ( /*line :2125:33*/_Ciconst_SQLITE_OK /*line :2125:43*/) && rv != ( /*line :2125:54*/_Ciconst_SQLITE_DONE /*line :2125:66*/) {
		err := s.c.lastError()
		func() _Ctype_int{ _cgo0 := /*line :2127:19*/s.s; _cgoCheckPointer(_cgo0, nil); return /*line :2127:23*/_Cfunc_sqlite3_reset(_cgo0); }()
