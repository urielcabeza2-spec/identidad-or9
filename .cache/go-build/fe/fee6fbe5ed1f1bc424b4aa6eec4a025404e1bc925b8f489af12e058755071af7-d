// Close the statement.
func (s *SQLiteStmt) Close() error {
	s.mu.Lock()
	defer s.mu.Unlock()
	if s.closed {
		return nil
	}
	s.closed = true
	if !s.c.dbConnOpen() {
		return errors.New("sqlite statement with already closed database connection")
	}
	rv := func() _Ctype_int{ _cgo0 := /*line :1938:27*/s.s; _cgoCheckPointer(_cgo0, nil); return /*line :1938:31*/_Cfunc_sqlite3_finalize(_cgo0); }()
	s.s = nil
	if rv != ( /*line :1940:11*/_Ciconst_SQLITE_OK /*line :1940:21*/) {
		return s.c.lastError()
	}
	s.c = nil
	runtime.SetFinalizer(s, nil)
	return nil
}

