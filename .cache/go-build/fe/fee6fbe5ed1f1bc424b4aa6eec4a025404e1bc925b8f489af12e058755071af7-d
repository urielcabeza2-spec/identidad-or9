			func() _Ctype_int{ _cgo0 := /*line :1726:23*/db; _cgoCheckPointer(_cgo0, nil); return /*line :1726:26*/_Cfunc_sqlite3_close_v2(_cgo0); }()
			return nil, err
		}
	}

